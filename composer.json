{
    "name": "flipboxstudio/php-flip",
    "description": "Clean architecture from Flipbox.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/lumen-framework": "^5.4",
        "vlucas/phpdotenv": "~2.2",
        "simple-validator/simple-validator": "^1.0",
        "sabre/event": "^5.0",
        "spatie/array-to-xml": "^2.5",
        "flipbox/lumen-generator": "^5.4",
        "phpmailer/phpmailer": "^5.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test"
        },
        "classmap": [
            "database/"
        ]
    },
    "scripts": {
        "fixer": [
            "php-cs-fixer fix --rules=@Symfony app",
            "php-cs-fixer fix --rules=@Symfony core",
            "php-cs-fixer fix --rules=@Symfony test"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
